<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="443.0" prefWidth="520.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.RegisterController">
   <children>
      <AnchorPane prefHeight="82.0" prefWidth="520.0" style="-fx-background-color: #0302D9;">
         <children>
            <Label layoutX="30.0" layoutY="32.0" prefHeight="27.0" prefWidth="139.0" text="User Registration" textFill="WHITE">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
      <Label layoutX="64.0" layoutY="87.0" prefHeight="25.0" prefWidth="336.0" text="Firstname">
         <font>
            <Font size="13.0" />
         </font>
      </Label>
      <TextField fx:id="firstnameTextField" layoutX="64.0" layoutY="113.0" prefHeight="26.0" prefWidth="220.0" promptText="Firstname" />
      <Label layoutX="64.0" layoutY="139.0" prefHeight="25.0" prefWidth="336.0" text="Lastname">
         <font>
            <Font size="13.0" />
         </font>
      </Label>
      <TextField fx:id="lastnameTextField" layoutX="64.0" layoutY="165.0" prefHeight="26.0" prefWidth="220.0" promptText="Lastname" />
      <Label layoutX="64.0" layoutY="191.0" prefHeight="25.0" prefWidth="336.0" text="Username">
         <font>
            <Font size="13.0" />
         </font>
      </Label>
      <TextField fx:id="usernameTextField" layoutX="64.0" layoutY="217.0" prefHeight="26.0" prefWidth="220.0" promptText="Username" />
      <Label layoutX="64.0" layoutY="303.0" prefHeight="25.0" prefWidth="336.0" text="Password">
         <font>
            <Font size="13.0" />
         </font>
      </Label>
      <Label layoutX="66.0" layoutY="243.0" prefHeight="25.0" prefWidth="336.0" text="Position">
         <font>
            <Font size="13.0" />
         </font>
      </Label>
      <Button fx:id="registerButton" layoutX="64.0" layoutY="367.0" mnemonicParsing="false" onAction="#registerButtonOnAction" prefWidth="220.0" text="Register">
         <font>
            <Font size="13.0" />
         </font>
      </Button>
      <Button fx:id="closeButton" layoutX="64.0" layoutY="403.0" mnemonicParsing="false" onAction="#closeButtonOnAction" prefWidth="220.0" text="Return to Signup">
         <font>
            <Font size="13.0" />
         </font>
      </Button>
      <Label fx:id="registrationMessageLabel" layoutX="311.0" layoutY="372.0" prefHeight="18.0" prefWidth="197.0" />
      <PasswordField fx:id="setPasswordField" layoutX="63.0" layoutY="329.0" prefHeight="26.0" prefWidth="220.0" />
      <TextField fx:id="positionTextField" layoutX="64.0" layoutY="269.0" prefHeight="26.0" prefWidth="220.0" promptText="Position" />
   </children>
</AnchorPane>
